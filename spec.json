{
  "name": "sample-monorepo",
  "services": [
    {
      "name": "api",
      "image": {
        "registry_type": "DOCR",
        "repository": "registry.digitalocean.com/\u003cmy-registry\u003e/\u003cmy-image\u003e",
        "tag": "test2"
      },
      "source_dir": "api",
      "environment_slug": "go",
      "routes": [{ "path": "/api" }]
    },
    {
      "name": "webhook",
      "image": {
        "registry_type": "DOCR",
        "repository": "registry.digitalocean.com/\u003cmy-registry\u003e/\u003cmy-image\u003e",
        "tag": "latest"
      },
      "source_dir": "webhook",
      "environment_slug": "go",
      "routes": [{ "path": "/webhook" }]
    }
  ],
  "static_sites": [
    {
      "name": "frontend",
      "source_dir": "frontend",
      "environment_slug": "html",
      "index_document": "hello.html",
      "error_document": "error.html",
      "routes": [{ "path": "/" }]
    },
    {
      "name": "landing",
      "dockerfile_path": "landing/Dockerfile",
      "source_dir": "landing",
      "output_dir": "/app",
      "error_document": "error.html",
      "routes": [{ "path": "/landing" }]
    }
  ],
  "workers": [
    {
      "name": "worker",
      "github": {
        "repo": "digitalocean/sample-monorepo",
        "branch": "main",
        "deploy_on_push": true
      },
      "dockerfile_path": "worker/Dockerfile",
      "run_command": "sh -c \"cat ./ok \u0026\u0026 sleep infinity\"",
      "source_dir": "worker"
    }
  ]
}
